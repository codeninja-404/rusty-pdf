{
  "name": "rusty-pdf",
  "version": "1.1.0",
  "type": "module",
  "description": "A React PDF reader powered by Rust/WASM with theming support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build:wasm": "wasm-pack build --target web --out-dir src/wasm",
    "build:js": "vite build",
    "build": "npm run build:wasm && npm run build:js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "pdf",
    "react",
    "wasm",
    "rust",
    "pdf-viewer",
    "webassembly"
  ],
  "author": "codeninja-404",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeninja-404/rusty-pdf.git"
  },
  "homepage": "https://github.com/codeninja-404/rusty-pdf#readme",
  "bugs": {
    "url": "https://github.com/codeninja-404/rusty-pdf/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4"
  }
}
